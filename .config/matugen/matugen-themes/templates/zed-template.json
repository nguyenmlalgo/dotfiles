{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "name": "nguyenmachine",
  "author": "Generated by matugen",
  "themes": [
    {
      "name": "nguyenmachine",
      "appearance": "dark",
      "style": {
        "title_bar.background": "{{colors.surface.default.hex}}",
        "border": "{{colors.surface.default.hex}}",
        "tab_bar.background": "{{colors.surface.default.hex}}",
        "panel.background": "{{colors.surface.default.hex}}",
        "status_bar.background": "{{colors.surface.default.hex}}",
        "toolbar.background": "{{colors.surface.default.hex}}",
        "border.variant": "{{colors.surface.default.hex}}",
        
        "editor.background": "{{colors.surface.default.hex}}",
        "editor.foreground": "{{colors.on_surface.default.hex}}",
        "editor.gutter.background": "{{colors.surface.default.hex}}",
        "editor.line_number": "{{colors.on_surface_variant.default.hex}}",
        "editor.active_line_number": "{{colors.primary.default.hex}}",
        "editor.active_line.background": "{{colors.surface.default.hex}}",
        "editor.indent_guide": "{{colors.outline.default.hex}}",
        "editor.indent_guide_active": "{{colors.primary.default.hex}}",
        
        "tab.inactive_background": "{{colors.surface.default.hex}}",
        "tab.active_background": "{{colors.surface.default.hex}}",
        
        "text": "{{colors.on_surface.default.hex}}",
        "text.muted": "{{colors.on_surface_variant.default.hex}}",
        
        "scrollbar.track.background": "{{colors.surface.default.hex}}",
        "scrollbar.track.border": "{{colors.surface.default.hex}}",
        "scrollbar.thumb.background": "{{colors.outline.default.hex}}",
        "scrollbar.thumb.hover_background": "{{colors.primary.default.hex}}",
        "scrollbar.thumb.active_background": "{{colors.primary.default.hex}}",
        
        "element.selected": "{{colors.primary_container.default.hex}}",
        "panel.focused_border": "{{colors.primary.default.hex}}",
        
        "warning.background": "{{colors.error_container.default.hex}}",
        "warning.border": "{{colors.error.default.hex}}",
        "error.background": "{{colors.error_container.default.hex}}",
        "error.border": "{{colors.error.default.hex}}",
        
        "hint": "{{colors.outline.default.hex}}",
        "editor.wrap_guide": "{{colors.outline.default.hex}}",
        "editor.active_wrap_guide": "{{colors.primary.default.hex}}",
        
        "editor.selection.background": "{{colors.primary_container.default.hex}}",
        "editor.selection.foreground": "{{colors.on_primary_container.default.hex}}",
        
        "predictive": "{{colors.outline.default.hex}}",
        "predictive.background": "{{colors.surface.default.hex}}",
        
        "modified": "{{colors.yellow.default.hex}}",
        "created": "{{colors.green.default.hex}}",
        "deleted": "{{colors.red.default.hex}}",
        "renamed": "{{colors.blue.default.hex}}",
        "conflict": "{{colors.red.default.hex}}",
        
        "syntax": {
          "variable": { 
            "color": "{{colors.on_surface.default.hex}}" 
          },
          "punctuation": { 
            "color": "{{colors.outline.default.hex}}" 
          },
          "keyword": { 
            "color": "{{colors.primary.default.hex}}" 
          },
          "function": { 
            "color": "{{colors.blue.default.hex}}" 
          },
          "string": { 
            "color": "{{colors.tertiary.default.hex}}" 
          },
          "string.special": { 
            "color": "{{colors.tertiary.default.hex}}" 
          },
          "string.special.symbol": { 
            "color": "{{colors.tertiary.default.hex}}" 
          },
          "operator": { 
            "color": "{{colors.primary.default.hex}}" 
          },
          "property": { 
            "color": "{{colors.cyan.default.hex}}" 
          },
          "type": { 
            "color": "{{colors.yellow.default.hex}}" 
          },
          "title": { 
            "color": "{{colors.primary.default.hex}}", 
            "font_weight": 600 
          },
          "constructor": { 
            "color": "{{colors.secondary.default.hex}}" 
          },
          "comment": { 
            "color": "{{colors.outline.default.hex}}", 
            "font_style": "italic" 
          },
          "suggestion": { 
            "color": "{{colors.outline.default.hex}}" 
          },
          "predictive": { 
            "color": "{{colors.outline.default.hex}}" 
          },
          "variant": { 
            "color": "{{colors.outline.default.hex}}" 
          },
          "constant": { 
            "color": "{{colors.red.default.hex}}" 
          },
          "number": { 
            "color": "{{colors.red.default.hex}}" 
          },
          "tag": { 
            "color": "{{colors.blue.default.hex}}", 
            "font_style": "italic" 
          }
        },
        
        "terminal.ansi.black": "{{colors.surface.default.hex}}",
        "terminal.ansi.red": "{{colors.red.default.hex}}",
        "terminal.ansi.green": "{{colors.green.default.hex}}",
        "terminal.ansi.yellow": "{{colors.yellow.default.hex}}",
        "terminal.ansi.blue": "{{colors.blue.default.hex}}",
        "terminal.ansi.magenta": "{{colors.tertiary.default.hex}}",
        "terminal.ansi.cyan": "{{colors.cyan.default.hex}}",
        "terminal.ansi.white": "{{colors.on_surface.default.hex}}",
        "terminal.ansi.bright_black": "{{colors.outline.default.hex}}",
        "terminal.ansi.bright_red": "{{colors.red.default.hex}}",
        "terminal.ansi.bright_green": "{{colors.green.default.hex}}",
        "terminal.ansi.bright_yellow": "{{colors.yellow.default.hex}}",
        "terminal.ansi.bright_blue": "{{colors.blue.default.hex}}",
        "terminal.ansi.bright_magenta": "{{colors.tertiary.default.hex}}",
        "terminal.ansi.bright_cyan": "{{colors.cyan.default.hex}}",
        "terminal.ansi.bright_white": "{{colors.on_surface.default.hex}}",
        
        "background.appearance": "opaque"
      }
    }
  ]
}
